<Window x:Class="ProcessTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProcessTest"
        xmlns:ctrls="clr-namespace:ProcessTest.Controls"
        xmlns:cables="clr-namespace:ProcessTest.Controls.Cables"
        xmlns:vm="clr-namespace:ProcessTest.ViewModels"
        xmlns:view="clr-namespace:ProcessTest.Views"
        xmlns:conv="clr-namespace:ProcessTest.Converters"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
    
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <vm:WorkflowViewModel/>
    </Window.DataContext>
    <Window.Resources>
        <conv:OffsetToMarginMulticonverter x:Key="OffsetToMarginMulticonverter"/>
    </Window.Resources>
    <Grid>
        <view:NodesPresenter x:Name="MyNodePresenter">
            <view:NodesPresenter.DataContext>
                <vm:WorkflowViewModel/>
            </view:NodesPresenter.DataContext>
        </view:NodesPresenter>
    </Grid>

</Window>
